{
  "include": ["./**/*"],
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
  },
  "exclude": [
    "node_modules",
    "out",
    ".next",
  ]
}
